# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# GET         /dataset-manager/swagger.json controllers.Swagger.swaggerSpec(name="dataset_manager.yaml")
GET         /spec/dataset_manager.yaml   controllers.Swagger.swaggerSpec(name="dataset_manager.yaml")

GET         /dataset-manager              controllers.Assets.versioned(path="/public/swagger-ui", file: Asset = "index.html")

GET         /index.html                    controllers.Assets.versioned(path="/public/swagger-ui", file: Asset = "index.html")

GET         /o2c.html                      controllers.Assets.versioned(path="/public/swagger-ui", file: Asset = "o2c.html")

GET         /favicon.ico    controllers.Assets.versioned(path="/public/swagger-ui/images", file: Asset = "favicon.ico")

GET         /api/*file                     controllers.Assets.versioned(path="/public/swagger-ui", file: Asset)

->          /dataset-manager/v1           dataset_manager.yaml.Routes

### NoDocs ###
GET         /docs/swagger-ui/*file    controllers.Assets.at(path:String="/public/lib/swagger-ui", file:String)

### NoDocs ###
GET         /*file             controllers.Assets.versioned(path="/public/swagger-ui", file: Asset)

# GET         /dataset-manager/storage/              controllers.PhysicalDatasetController.getDataset(uri:String, format:String, limit:Option[Int], chunk_size:Option[Int])